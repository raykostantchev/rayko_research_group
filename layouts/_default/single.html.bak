{{- define "main" -}}

<article class="article" {{- if .Param "autonumbering" }} autonumbering {{- end }}>

  {{ partial "page_header" . }}

  <div class="article-container">

    <div class="article-style">
      {{ .Content }}
    </div>

    {{ partial "page_footer" . }}

  </div>
</article>


{{- end -}}
